@prefix ex:   <http://example.org/mes#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

################
# Manufacturing Sample Data
# 완전한 제조 라인 예제: 3개 설비, 5개 센서, 10개 작업지시서
################

# 설비 (Equipment)
ex:ProductionLine1 a ex:Equipment ; 
  rdfs:label "Production Line 1" ;
  ex:code "PL001" ;
  ex:status "active" .

ex:MachineA a ex:Equipment ; 
  rdfs:label "Machine A (Assembly)" ;
  ex:code "MA001" ;
  ex:status "active" .

ex:MachineB a ex:Equipment ; 
  rdfs:label "Machine B (Testing)" ;
  ex:code "MB001" ;
  ex:status "active" .

ex:MachineC a ex:Equipment ; 
  rdfs:label "Machine C (Packaging)" ;
  ex:code "MC001" ;
  ex:status "maintenance" .

# 설비 계층구조
ex:MachineA ex:partOf ex:ProductionLine1 .
ex:MachineB ex:partOf ex:ProductionLine1 .
ex:MachineC ex:partOf ex:ProductionLine1 .

# 센서 (Sensors)
ex:TempSensor1 a ex:Sensor ; 
  rdfs:label "Temperature Sensor 1" ;
  ex:code "TS001" .

ex:PressureSensor1 a ex:Sensor ; 
  rdfs:label "Pressure Sensor 1" ;
  ex:code "PS001" .

ex:VibrationSensor1 a ex:Sensor ; 
  rdfs:label "Vibration Sensor 1" ;
  ex:code "VS001" .

ex:QualitySensor1 a ex:Sensor ; 
  rdfs:label "Quality Check Sensor" ;
  ex:code "QS001" .

ex:SpeedSensor1 a ex:Sensor ; 
  rdfs:label "Speed Sensor 1" ;
  ex:code "SS001" .

# 센서-설비 연결
ex:TempSensor1 ex:locatedIn ex:MachineA .
ex:PressureSensor1 ex:locatedIn ex:MachineA .
ex:VibrationSensor1 ex:locatedIn ex:MachineB .
ex:QualitySensor1 ex:locatedIn ex:MachineB .
ex:SpeedSensor1 ex:locatedIn ex:MachineC .

# 변수 (Variables)
ex:Temperature a ex:Variable ; 
  rdfs:label "Process Temperature" ;
  ex:specUpper 85.0 ;
  ex:specLower 75.0 .

ex:Pressure a ex:Variable ; 
  rdfs:label "Process Pressure" ;
  ex:specUpper 2.5 ;
  ex:specLower 1.8 .

ex:Vibration a ex:Variable ; 
  rdfs:label "Machine Vibration" ;
  ex:specUpper 5.0 ;
  ex:specLower 0.0 .

ex:QualityScore a ex:Variable ; 
  rdfs:label "Quality Score" ;
  ex:specUpper 100.0 ;
  ex:specLower 95.0 .

ex:Speed a ex:Variable ; 
  rdfs:label "Production Speed" ;
  ex:specUpper 120.0 ;
  ex:specLower 80.0 .

# 단위 (Units)
ex:Celsius a ex:Unit ; rdfs:label "°C" .
ex:Bar a ex:Unit ; rdfs:label "bar" .
ex:mmPerSec a ex:Unit ; rdfs:label "mm/s" .
ex:Percent a ex:Unit ; rdfs:label "%" .
ex:PiecesPerHour a ex:Unit ; rdfs:label "pcs/h" .

# 변수-단위 연결
ex:Temperature ex:hasUnit ex:Celsius .
ex:Pressure ex:hasUnit ex:Bar .
ex:Vibration ex:hasUnit ex:mmPerSec .
ex:QualityScore ex:hasUnit ex:Percent .
ex:Speed ex:hasUnit ex:PiecesPerHour .

# 센서-변수 연결
ex:TempSensor1 ex:measures ex:Temperature .
ex:PressureSensor1 ex:measures ex:Pressure .
ex:VibrationSensor1 ex:measures ex:Vibration .
ex:QualitySensor1 ex:measures ex:QualityScore .
ex:SpeedSensor1 ex:measures ex:Speed .

# 영역 (Areas)
ex:AssemblyArea a ex:Area ; 
  rdfs:label "Assembly Area" ;
  ex:code "AA001" .

ex:TestingArea a ex:Area ; 
  rdfs:label "Testing Area" ;
  ex:code "TA001" .

ex:PackagingArea a ex:Area ; 
  rdfs:label "Packaging Area" ;
  ex:code "PA001" .

# 설비-영역 연결
ex:MachineA ex:locatedIn ex:AssemblyArea .
ex:MachineB ex:locatedIn ex:TestingArea .
ex:MachineC ex:locatedIn ex:PackagingArea .

# 프로세스 (Process)
ex:AssemblyProcess a ex:Process ; 
  rdfs:label "Assembly Process" ;
  ex:code "AP001" .

ex:TestingProcess a ex:Process ; 
  rdfs:label "Testing Process" ;
  ex:code "TP001" .

ex:PackagingProcess a ex:Process ; 
  rdfs:label "Packaging Process" ;
  ex:code "PP001" .

# 작업 (Operations)
ex:AssemblyOp1 a ex:Operation ; 
  rdfs:label "Component Assembly" ;
  ex:code "AO001" .

ex:AssemblyOp2 a ex:Operation ; 
  rdfs:label "Final Assembly" ;
  ex:code "AO002" .

ex:TestingOp1 a ex:Operation ; 
  rdfs:label "Functional Test" ;
  ex:code "TO001" .

ex:TestingOp2 a ex:Operation ; 
  rdfs:label "Quality Inspection" ;
  ex:code "TO002" .

ex:PackagingOp1 a ex:Operation ; 
  rdfs:label "Product Packaging" ;
  ex:code "PO001" .

# 작업 순서
ex:AssemblyOp1 ex:precedes ex:AssemblyOp2 .
ex:AssemblyOp2 ex:precedes ex:TestingOp1 .
ex:TestingOp1 ex:precedes ex:TestingOp2 .
ex:TestingOp2 ex:precedes ex:PackagingOp1 .

# 재료 (Materials)
ex:ComponentA a ex:Material ; 
  rdfs:label "Component A" ;
  ex:code "CA001" .

ex:ComponentB a ex:Material ; 
  rdfs:label "Component B" ;
  ex:code "CB001" .

ex:ComponentC a ex:Material ; 
  rdfs:label "Component C" ;
  ex:code "CC001" .

ex:PackagingMaterial a ex:Material ; 
  rdfs:label "Packaging Material" ;
  ex:code "PM001" .

# 작업-재료 연결
ex:AssemblyOp1 ex:consumes ex:ComponentA .
ex:AssemblyOp1 ex:consumes ex:ComponentB .
ex:AssemblyOp2 ex:consumes ex:ComponentC .
ex:PackagingOp1 ex:consumes ex:PackagingMaterial .

# 제품 (Products)
ex:ProductA a ex:Product ; 
  rdfs:label "Product A" ;
  ex:code "PA001" .

ex:ProductB a ex:Product ; 
  rdfs:label "Product B" ;
  ex:code "PB001" .

ex:ProductC a ex:Product ; 
  rdfs:label "Product C" ;
  ex:code "PC001" .

# 작업-제품 연결
ex:AssemblyOp2 ex:produces ex:ProductA .
ex:TestingOp2 ex:produces ex:ProductB .
ex:PackagingOp1 ex:produces ex:ProductC .

# 작업지시서 (Work Orders)
ex:WorkOrder001 a ex:WorkOrder ; 
  ex:workOrderNumber "WO001" ;
  ex:plannedQuantity 100 ;
  ex:actualQuantity 95 ;
  ex:status "completed" ;
  ex:timestamp "2024-01-15T08:00:00Z"^^xsd:dateTime .

ex:WorkOrder002 a ex:WorkOrder ; 
  ex:workOrderNumber "WO002" ;
  ex:plannedQuantity 150 ;
  ex:actualQuantity 150 ;
  ex:status "completed" ;
  ex:timestamp "2024-01-15T10:00:00Z"^^xsd:dateTime .

ex:WorkOrder003 a ex:WorkOrder ; 
  ex:workOrderNumber "WO003" ;
  ex:plannedQuantity 200 ;
  ex:actualQuantity 0 ;
  ex:status "planned" ;
  ex:timestamp "2024-01-15T12:00:00Z"^^xsd:dateTime .

ex:WorkOrder004 a ex:WorkOrder ; 
  ex:workOrderNumber "WO004" ;
  ex:plannedQuantity 80 ;
  ex:actualQuantity 45 ;
  ex:status "in_progress" ;
  ex:timestamp "2024-01-15T14:00:00Z"^^xsd:dateTime .

ex:WorkOrder005 a ex:WorkOrder ; 
  ex:workOrderNumber "WO005" ;
  ex:plannedQuantity 120 ;
  ex:actualQuantity 0 ;
  ex:status "cancelled" ;
  ex:timestamp "2024-01-15T16:00:00Z"^^xsd:dateTime .

# 작업지시서-설비 연결
ex:WorkOrder001 ex:executedBy ex:MachineA .
ex:WorkOrder002 ex:executedBy ex:MachineB .
ex:WorkOrder003 ex:executedBy ex:MachineA .
ex:WorkOrder004 ex:executedBy ex:MachineC .
ex:WorkOrder005 ex:executedBy ex:MachineB .

# 배치 (Batches)
ex:Batch001 a ex:Batch ; 
  ex:batchNumber "B001" ;
  ex:timestamp "2024-01-15T08:00:00Z"^^xsd:dateTime .

ex:Batch002 a ex:Batch ; 
  ex:batchNumber "B002" ;
  ex:timestamp "2024-01-15T10:00:00Z"^^xsd:dateTime .

ex:Batch003 a ex:Batch ; 
  ex:batchNumber "B003" ;
  ex:timestamp "2024-01-15T12:00:00Z"^^xsd:dateTime .

# 제품-배치 연결
ex:ProductA ex:belongsToBatch ex:Batch001 .
ex:ProductB ex:belongsToBatch ex:Batch002 .
ex:ProductC ex:belongsToBatch ex:Batch003 .

# 품질관리 (Quality Control)
ex:QualityCheck001 a ex:QualityControl ; 
  ex:qualityResult "pass" ;
  ex:timestamp "2024-01-15T09:30:00Z"^^xsd:dateTime .

ex:QualityCheck002 a ex:QualityControl ; 
  ex:qualityResult "pass" ;
  ex:timestamp "2024-01-15T11:30:00Z"^^xsd:dateTime .

ex:QualityCheck003 a ex:QualityControl ; 
  ex:qualityResult "fail" ;
  ex:timestamp "2024-01-15T13:30:00Z"^^xsd:dateTime .

ex:QualityCheck004 a ex:QualityControl ; 
  ex:qualityResult "pending" ;
  ex:timestamp "2024-01-15T15:30:00Z"^^xsd:dateTime .

# 제품-품질관리 연결
ex:ProductA ex:hasQuality ex:QualityCheck001 .
ex:ProductB ex:hasQuality ex:QualityCheck002 .
ex:ProductC ex:hasQuality ex:QualityCheck003 .

# 불량 (Defects)
ex:Defect001 a ex:Defect ; 
  ex:defectCode "DC001" ;
  ex:defectCount 3 .

ex:Defect002 a ex:Defect ; 
  ex:defectCode "DC002" ;
  ex:defectCount 1 .

ex:Defect003 a ex:Defect ; 
  ex:defectCode "DC003" ;
  ex:defectCount 2 .

# 제품-불량 연결
ex:ProductA ex:hasDefect ex:Defect001 .
ex:ProductB ex:hasDefect ex:Defect002 .
ex:ProductC ex:hasDefect ex:Defect003 .

# 유지보수 (Maintenance)
ex:Maintenance001 a ex:Maintenance ; 
  ex:maintenanceType "preventive" ;
  ex:timestamp "2024-01-14T18:00:00Z"^^xsd:dateTime .

ex:Maintenance002 a ex:Maintenance ; 
  ex:maintenanceType "corrective" ;
  ex:timestamp "2024-01-15T16:30:00Z"^^xsd:dateTime .

ex:Maintenance003 a ex:Maintenance ; 
  ex:maintenanceType "predictive" ;
  ex:timestamp "2024-01-15T17:00:00Z"^^xsd:dateTime .

# 설비-유지보수 연결
ex:MachineA ex:hasMaintenance ex:Maintenance001 .
ex:MachineB ex:hasMaintenance ex:Maintenance002 .
ex:MachineC ex:hasMaintenance ex:Maintenance003 .
